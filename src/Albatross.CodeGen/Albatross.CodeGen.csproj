<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFramework>netstandard2.0</TargetFramework>
		<PackageId>Albatross.CodeGen</PackageId>
		<Version>2.0.0-prerelease3</Version>
		<Authors>Rushui Guan</Authors>
		<Product>Albatross Code Generator</Product>
		<Description>A customizable code generator engine with a Powershell tool that allows user to quickly setup composite generators and update the target file.</Description>
		<PackageReleaseNotes></PackageReleaseNotes>
		<PackageTags>code generator c# sql codegen powershell database csharp</PackageTags>
		<LangVersion>7.3</LangVersion>
		<PackageProjectUrl>https://github.com/RushuiGuan/CodeGen</PackageProjectUrl>
		<PackageLicenseFile>LICENSE</PackageLicenseFile>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<Copyright>Copyright © 2019</Copyright>
	</PropertyGroup>
    
	<ItemGroup>
		<PackageReference Include="Albatross.Database" Version="1.1.7" />
		<PackageReference Include="Autofac" Version="4.9.2" />
	</ItemGroup>
    
	<ItemGroup>
	  <None Include="..\..\LICENSE">
	    <Pack>True</Pack>
	    <PackagePath></PackagePath>
	  </None>
	</ItemGroup>
</Project>