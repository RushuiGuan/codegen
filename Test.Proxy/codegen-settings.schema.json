            {
              "type": "null"
            },
            {
              "$ref": "#/definitions/WebClientMethodSettings"
            }
          ]
        },
        "methodSettings": {
          "type": "object",
          "additionalProperties": {
            "$ref": "#/definitions/WebClientMethodSettings"
          }
        },
        "constructorSettings": {
          "type": "object",
          "additionalProperties": {
            "$ref": "#/definitions/WebClientConstructorSettings"
          }
        }
      }
