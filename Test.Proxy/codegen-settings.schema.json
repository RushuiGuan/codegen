        },
        "sourcePathRelatedToEntryFile": {
          "type": "string"
        },
        "namespaceModuleMapping": {
          "type": "object",
          "additionalProperties": {
            "type": "string"
          }
        },
        "typeMapping": {
          "type": "object",
          "additionalProperties": {
            "type": "string"
          }
        }
      }
    },
    "PythonWebClientSettings": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
