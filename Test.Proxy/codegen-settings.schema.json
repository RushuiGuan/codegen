          }
        }
      }
    },
    "WebClientConstructorSettings": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "compressionEncoding": {
          "type": [
            "null",
            "string"
          ]
        },
        "omit": {
          "type": "boolean"
        },
        "customJsonSettings": {
          "type": [
            "null",
            "string"
          ]
        }
      }
    },
    "CSharpWebClientSettings": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "namespace": {
          "type": "string"
