    "WebClientMethodSettings": {
      "type": "object",
      "additionalProperties": false
    },
    "SymbolFilterPatterns": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "exclude": {
          "type": [
            "null",
            "string"
          ]
        },
        "include": {
          "type": [
            "null",
            "string"
          ]
        },
        "hasValue": {
          "type": "boolean"
        }
      }
