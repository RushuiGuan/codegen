        "namespaceModuleMapping": {
          "type": "object",
          "additionalProperties": {
            "type": "string"
          }
        },
        "typeMapping": {
          "type": "object",
          "additionalProperties": {
            "type": "string"
          }
        },
        "propertyNameMapping": {
          "type": "object",
          "additionalProperties": {
            "type": "string"
          }
        },
        "httpClientClassNameMapping": {
          "type": "object",
          "additionalProperties": {
            "type": "string"
          }
